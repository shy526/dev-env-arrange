{
    "name":"maven3",
    "variable":{
        "config":"${root}/config/setting.xml"
    },
    "check":"./mvn -v",
    "download":{
        "urlRoot":[
            "https://dlcdn.apache.org/maven/maven-3"
        ],
        "process":"common",
        "url":"${urlRoot}/${version}/binaries/apache-maven-${version}-bin.${osFormat}",
        "osFormat": {
            "os-win-format": "zip",
            "os-linux-format": "tar.gz"
        }
    },
    "operate":[
        "xml:${config} localRepository ${root}/localRepository += mirrors.mirror.id=nexus-aliyun + mirrors.mirror.mirrorOf=central + mirrors.mirror.name=\"Nexus aliyun\" mirrors.mirror.url=http://maven.aliyun.com/nexus/content/groups/public +",
        "file:${config} ${userHome}/.m2/config ->" ,
        "sysEnv:MAVEN_HOME_SHY ${root} = path ${PATH} ;%MAVEN_HOME_SHY%/bin +"
        ]
}