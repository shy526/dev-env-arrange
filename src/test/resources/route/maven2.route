{
    "name": "maven2",
    "variable": {
        "config": "${root}/config/setting.xml"
    },
    "check": "./mvn -v",
    "download": {
        "urlRoot": [
            "http://archive.apache.org/dist/maven/binaries"
        ],
        "process": "common",
        "url": "${urlRoot}/${version}/binaries/apache-maven-${version}-bin.${osFormat}",
        "osFormat": {
            "os-win-format": "zip",
            "os-linux-format": "tar.gz",
        }
    }
}